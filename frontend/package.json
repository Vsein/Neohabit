{
  "name": "neohabit-frontend",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "vite build --watch",
    "start": "REACT_APP_STAGE=dev vite serve --open",
    "build": "REACT_APP_STAGE=prod vite build --emptyOutDir",
    "lint": "eslint --ext .js,.jsx src/ --fix",
    "prettier": "prettier . --write",
    "obfuscate": "find dist -type f -name 'index-*.js' -print0 | xargs -0 -I {} terser {} --compress --mangle --output {}",
    "ship": "GENERATE_SOURCEMAP=false npm run build && npm run obfuscate",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal-2023 public/favicon-light.svg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vsein/Neohabit.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Vsein/Neohabit/issues"
  },
  "homepage": "https://github.com/Vsein/Neohabit#readme",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@vite-pwa/assets-generator": "^1.0.2",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "msw": "^2.12.7",
    "prettier": "3.6.2",
    "sass": "^1.94.2",
    "vite": "^7.2.4",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pwa": "^1.1.0"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "@reduxjs/toolkit": "^2.11.0",
    "date-fns": "^4.1.0",
    "localforage": "^1.10.0",
    "react": "^19.2.0",
    "react-colorful": "^5.6.1",
    "react-datepicker": "^8.10.0",
    "react-dom": "^19.2.0",
    "react-final-form": "^7.0.0",
    "react-hotkeys-hook": "^5.2.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.6",
    "redux-persist": "^6.0.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
